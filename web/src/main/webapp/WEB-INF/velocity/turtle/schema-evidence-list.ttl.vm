#if($prefix)
#parse("turtle/prefix.ttl.vm")
#end

#
# Define evidences
#

:Evidence
      rdf:type owl:Class ;
      rdfs:label "Evidence"^^xsd:string ;
      rdfs:comment "Evidence"^^xsd:string ;
      rdfs:isDefinedBy <http://nextprot.org/rdf> ;
      .

:evidence
      a rdf:Property, owl:ObjectProperty ;
      rdfs:label "evidence"^^xsd:string ;
      rdfs:comment "Links an annotation with the evidence supporting it"^^xsd:string ;
      rdfs:isDefinedBy <http://nextprot.org/rdf> ;
      rdfs:domain :AnnotationType ;
      rdfs:range :Evidence ;
      .

:negativeEvidence
      a rdf:Property, owl:ObjectProperty ;
      rdfs:label "evidence"^^xsd:string ;
      rdfs:comment "Links an annotation with an evidence unsupporting it"^^xsd:string ;
      rdfs:isDefinedBy <http://nextprot.org/rdf> ;
      rdfs:domain :AnnotationType ;
      rdfs:range :Evidence ;
      .

:assignedBy
      a rdf:Property, owl:FunctionalProperty, owl:ObjectProperty ;
      rdfs:label "assigned by"^^xsd:string ;
      rdfs:comment "Links an evidence to its source"^^xsd:string ;
      rdfs:isDefinedBy <http://nextprot.org/rdf> ;
      rdfs:domain :Evidence ;
      rdfs:range :Source ;
      .
      
 
 :AntibodyIntegrationLevel
      rdf:type owl:Class ;
      rdfs:label "Antibody integration level"^^xsd:string ;
      rdfs:comment "Antibody integration level"^^xsd:string ;
      rdfs:isDefinedBy <http://nextprot.org/rdf> ;
      owl:equivalentClass [ 
        a owl:Class ;
      	owl:oneOf (
	      	:Integrated 
	      	:Selected 
	      	:Single
        ) 
      ];
      .

 :integrationLevel
      a rdf:Property, owl:ObjectProperty ;
      rdfs:label "integration level"^^xsd:string ;
      rdfs:comment "antibody integration level"^^xsd:string ;
      rdfs:isDefinedBy <http://nextprot.org/rdf> ;
      rdfs:domain :Evidence ;
      rdfs:range :AntibodyIntegrationLevel ;
       
 
:Integrated
	  rdf:type owl:Thing, owl:NamedIndividual, :AntibodyIntegrationLevel ;
      rdfs:label "Integrated"^^xsd:string ;
      rdfs:comment "Integrated"^^xsd:string ;
      rdfs:isDefinedBy <http://nextprot.org/rdf> ;
      .

:Selected
	  rdf:type owl:Thing, owl:NamedIndividual, :AntibodyIntegrationLevel ;
      rdfs:label "Selected"^^xsd:string ;
      rdfs:comment "Selected"^^xsd:string ;
      rdfs:isDefinedBy <http://nextprot.org/rdf> ;
      .

:Single
	  rdf:type owl:Thing, owl:NamedIndividual, :AntibodyIntegrationLevel ;
      rdfs:label "Single"^^xsd:string ;
      rdfs:comment "Single"^^xsd:string ;
      rdfs:isDefinedBy <http://nextprot.org/rdf> ;
      .
      
      
