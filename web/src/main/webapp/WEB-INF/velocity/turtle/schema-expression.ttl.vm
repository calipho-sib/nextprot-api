#
# Define Expresion specialisation
# 

:ExpressionLevel
      rdf:type owl:Class ;
      rdfs:label "Expression level"^^xsd:string ;
      rdfs:comment "Expression levels detected during experiment"^^xsd:string ;
      rdfs:isDefinedBy <http://nextprot.org/rdf> ;
      :sourceFile "schema-expression.ttl.vm"^^xsd:string ;      
      .
      
:expressionLevel
    rdfs:isDefinedBy <http://nextprot.org/rdf> ;
    :sourceFile "additional-properties-def.ttl.vm"^^xsd:string ;
    rdfs:label "expression level"^^xsd:string ;
    rdf:type  owl:ObjectProperty ;
    rdfs:domain :Evidence ;
    rdfs:range [ a owl:Class ; owl:unionOf (:PositiveExpressionLevel :NegativeExpressionLevel) ; ] ;
    . 
    
:PositiveExpressionLevel
      rdf:type owl:Class ;
      rdfs:subClassOf :ExpressionLevel ;
      rdfs:label "Positive expression level"^^xsd:string ;
      rdfs:comment "Class of positive expression level"^^xsd:string ;
      rdfs:isDefinedBy <http://nextprot.org/rdf> ;
      :sourceFile "schema-expression.ttl.vm"^^xsd:string ;
      owl:equivalentClass [ 
        a owl:Class ;
      	owl:oneOf ( :Positive :High :Medium :Low )
      ];
      .

:NegativeExpressionLevel
      rdf:type owl:Class ;
      rdfs:subClassOf :ExpressionLevel ;
      rdfs:label "Negative expression level"^^xsd:string ;
      rdfs:comment "Class of negative level of expression"^^xsd:string ;
      rdfs:isDefinedBy <http://nextprot.org/rdf> ;
      :sourceFile "schema-expression.ttl.vm"^^xsd:string ;
      owl:equivalentClass [ 
        a owl:Class ;
      	owl:oneOf ( :Negative )
      ];
      .

:Positive
      rdf:type owl:Thing, owl:NamedIndividual, :PositiveExpressionLevel ;
      rdfs:label "Expression detected"^^xsd:string ;
      rdfs:comment "Positive level of expression"^^xsd:string ;
      rdfs:isDefinedBy <http://nextprot.org/rdf> ;
      :sourceFile "schema-expression.ttl.vm"^^xsd:string ;
      .

:High
      rdf:type owl:Thing, owl:NamedIndividual, :PositiveExpressionLevel ;
      rdfs:label "High expression detected"^^xsd:string ;
      rdfs:comment "High level of expression"^^xsd:string ;
      rdfs:isDefinedBy <http://nextprot.org/rdf> ;
      :sourceFile "schema-expression.ttl.vm"^^xsd:string ;
      .

:Medium
      rdf:type owl:Thing, owl:NamedIndividual, :PositiveExpressionLevel ;
      rdfs:label "Medium expression detected"^^xsd:string ;
      rdfs:comment "Medium level of expression"^^xsd:string ;
      rdfs:isDefinedBy <http://nextprot.org/rdf> ;
      :sourceFile "schema-expression.ttl.vm"^^xsd:string ;
      .

:Low
      rdf:type owl:Thing, owl:NamedIndividual, :PositiveExpressionLevel ;
      rdfs:label "Low expression detected"^^xsd:string ;
      rdfs:comment "Low level of expression"^^xsd:string ;
      rdfs:isDefinedBy <http://nextprot.org/rdf> ;
      :sourceFile "schema-expression.ttl.vm"^^xsd:string ;
      .

:Negative
      rdf:type owl:Thing, owl:NamedIndividual, :NegativeExpressionLevel ;
      rdfs:label "No expression detected"^^xsd:string ;
      rdfs:comment "Negative level of expression"^^xsd:string ;
      rdfs:isDefinedBy <http://nextprot.org/rdf> ;
      :sourceFile "schema-expression.ttl.vm"^^xsd:string ;
      .        


