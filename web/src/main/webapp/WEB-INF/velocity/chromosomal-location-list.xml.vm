<chromosomal-location-list>
#foreach($chromosomalLocation in $entry.chromosomalLocations)
	<chromosomal-location chromosome='$chromosomalLocation.getChromosome()' 
		band='$chromosomalLocation.getBand()' strand='$chromosomalLocation.getStrand()' 
		accession='$chromosomalLocation.getAccession()' 
		geneStart='$chromosomalLocation.getFirstPosition()' geneEnd='$chromosomalLocation.getLastPosition()' 
	/>
#end
</chromosomal-location-list>
