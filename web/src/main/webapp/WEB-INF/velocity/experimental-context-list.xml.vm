<experimental-context-list>
#foreach($ctx in $entry.getExperimentalContexts())
    <experimental-context internal-id='$ctx.getContextId()' metadata-ac='$ctx.metadataAC'>
		#if($ctx.tissue)
		<cv-term type="tissue" terminology='$StringUtils.camelToKebabCase($ctx.getTissue().getOntology())' accession='$ctx.tissueAC'>
            <name>$ctx.getTissue().getName()</name>
        </cv-term>
		#end

		#if($ctx.developmentalStage)
            <cv-term type="developmental-stage" terminology='$StringUtils.camelToKebabCase($ctx.getDevelopmentalStage().getOntology())' accession='$ctx.developmentalStageAC'>
                <name>$ctx.getDevelopmentalStage().getName()</name>
            </cv-term>
		#end

		#if($ctx.detectionMethod)
		<cv-term type="detection-method" terminology='$StringUtils.camelToKebabCase($ctx.getDetectionMethod().getOntology())' accession='$ctx.detectionMethodAC'>
            <name>$ctx.getDetectionMethod().getName()</name>
        </cv-term>
		#end

		#if($ctx.cellLine)
            <cv-term type="cell-line" terminology='$StringUtils.camelToKebabCase($ctx.getCellLine().getOntology())' accession='$ctx.cellLineAC'>
                <name>$ctx.getCellLine().getName()</name>
            </cv-term>
		#end

		#if($ctx.disease)
            <cv-term type="disease" terminology='$StringUtils.camelToKebabCase($ctx.getDisease().getOntology())' accession='$ctx.diseaseAC'>
                <name>$ctx.getDisease().getName()</name>
            </cv-term>
		#end

		#if($ctx.organelle)
            <cv-term type="organelle" terminology='$StringUtils.camelToKebabCase($ctx.getOrganelle().getOntology())' accession='$ctx.organelleAC'>
                <name>$ctx.getOrganelle().getName()</name>
            </cv-term>
		#end
    </experimental-context>
#end
</experimental-context-list>
