#if(!$hierarchy.isEmpty())
#set($currentFamily=$hierarchy.remove(0))
<family level="$currentFamily.level">
    <cv-term accession="$currentFamily.accession" terminology="uniprot-family-cv">$currentFamily.name</cv-term>
    #if($currentFamily.region)
        <family-region>$currentFamily.region</family-region>
    #end
    #parse("family.xml.vm")
</family>
#end
